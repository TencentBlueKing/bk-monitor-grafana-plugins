{
  "name": "grafana-timeseries-plugin",
  "version": "1.0.0",
  "description": "Blueking Monitor Datasource Plugin For Grafana",
  "homepage": "http://grafana.org",
  "scripts": {
    "precommit": "yarn run lint-staged",
    "build": "webpack --config webpack/webpack.prod.conf.js --progress --colors",
    "dev": "webpack --config webpack/webpack.dev.conf.js --progress --colors",
    "watch": "webpack --config webpack/webpack.dev.conf.js --progress --colors",
    "lint": "eslint --fix --ext .ts,.tsx ./src",
    "prettier": "prettier --write ./ '!**/*.{css,scss,less}'"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{ts,tsx,js}": [
      "eslint --fix",
      "git add"
    ],
    "src/**/*.(vue|scss|css|sass)": [
      "stylelint --fix",
      "git add"
    ]
  },
  "author": "bkef",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.7.7",
    "@babel/preset-env": "^7.7.7",
    "@babel/preset-react": "^7.6.3",
    "@blueking/stylelint-config": "0.0.3",
    "@grafana/data": "7.2.2",
    "@grafana/runtime": "7.2.2",
    "@grafana/ui": "7.2.2",
    "@types/react": "^16.9.9",
    "@types/react-dom": "^16.9.9",
    "@types/react-transition-group": "^2.0.15",
    "@typescript-eslint/eslint-plugin": "^4.28.3",
    "@typescript-eslint/parser": "^4.28.3",
    "antd": "~4.20.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-import": "^1.13.0",
    "clean-webpack-plugin": "^0.1.19",
    "copy-webpack-plugin": "^5.1.1",
    "css-loader": "2.1.1",
    "eslint": "^7.31.0",
    "eslint-config-tencent": "^1.0.2",
    "eslint-plugin-codecc": "^0.0.4",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-prettier": "^3.4.0",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "html-loader": "^0.5.5",
    "less": "^3.11.1",
    "less-loader": "^5.0.0",
    "node-sass": "^4.13.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "prettier": "^2.3.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "sass-loader": "7.1.0",
    "style-loader": "^0.23.1",
    "terser-webpack-plugin": "^2.3.5",
    "ts-loader": "^4.4.1",
    "typescript": "^4.3.5",
    "webpack": "4.29.6",
    "webpack-cli": "3.2.3"
  },
  "engines": {
    "node": ">= 14"
  },
  "volta": {
    "node": "14.15.1"
  },
  "dependencies": {
    "@codemirror/autocomplete": "^0.18.8",
    "@codemirror/closebrackets": "^0.18.0",
    "@codemirror/commands": "^0.18.3",
    "@codemirror/comment": "^0.18.1",
    "@codemirror/highlight": "^0.18.4",
    "@codemirror/history": "^0.18.1",
    "@codemirror/language": "^0.18.2",
    "@codemirror/lint": "^0.18.4",
    "@codemirror/matchbrackets": "^0.18.0",
    "@codemirror/search": "^0.18.4",
    "@codemirror/state": "^0.18.7",
    "@codemirror/view": "^0.18.19",
    "codemirror-promql": "^0.16.0",
    "eslint-plugin-json": "^3.0.0",
    "husky": "^7.0.1",
    "lint-staged": "^11.1.1"
  }
}
